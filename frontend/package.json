{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "18.19.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite --config config/vite.config.js",
    "dev:prod": "vite --mode production --config config/vite.config.js",
    "build": "vite build --config config/vite.config.js && npm run generate-sitemap",
    "build:dev": "vite build --mode development --config config/vite.config.js",
    "build:prod": "vite build --mode production --config config/vite.config.js && npm run generate-sitemap",
    "generate-sitemap": "node config/sitemap/generateSitemap.js",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --config config/vite.config.js",
    "preview:prod": "vite preview --mode production --config config/vite.config.js"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@mapbox/polyline": "^1.2.1",
    "@tailwindcss/typography": "^0.5.15",
    "dompurify": "^3.1.7",
    "leaflet": "^1.9.4",
    "leaflet-image": "^0.4.0",
    "lodash": "^4.17.21",
    "mapbox-gl": "^3.7.0",
    "marked": "^14.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^4.9.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.27.0",
    "sitemap": "^8.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "vite": "^4.3.9"
  }
}
